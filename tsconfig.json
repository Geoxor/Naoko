{
  "compilerOptions": {
    "resolveJsonModule": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "moduleResolution": "node",
    "outDir": "dist",
    "declaration": false,
    "module": "ESNext",
    "rootDirs": ["./src"],
    "baseUrl": "./",
    "paths": {
      "src/*": ["src/*"]
    },
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./src", "./src/assets/*", "./src/config.naoko.json"],
  "exclude": ["src/setup.naoko.ts"]
}
